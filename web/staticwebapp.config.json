{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",               // all Vite-bundled static files
      "/hubspot-theme*.css",     // your HubSpot CSS at the site root
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml",
      "/manifest.webmanifest"
    ]
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "text/javascript"
  }
}
